	height: auto;
	line-height: 1;
	margin: 0;
	background-color: inherit;
	overflow: initial;
}
.cu-bar.tabbar.shop .action {
	width: 140rpx;
	flex: initial;
}
.cu-bar.tabbar .action.add-action {
